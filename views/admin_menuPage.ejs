<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Admin's Menu Page</title>
  <link rel="icon" type="image/x-icon" href="https://cdn.vectorstock.com/i/500p/24/33/creative-infinity-symbol-and-kitchen-equipement-vector-54242433.jpg">
  <!-- Bootstrap CSS -->
  <link href="./bootstrap.css" rel="stylesheet">
  <link href="./home.css" rel="stylesheet">
  <!-- Font Awesome -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons/font/bootstrap-icons.css" rel="stylesheet"/>
</head>

<body class="bg-light">

  <div style="z-index: 100;"><h1 style="color: black;">Admin's Side page</h1></div>

  <!-- Navbar -->
  <%- include('admin/admin_navbar')%>



  <!-- Content -->
  <div class="container mt-5 pt-5">

    <!-- Search Bar -->
    <div class="input-group my-4">
      <input type="text" class="form-control" id="search" placeholder="Search for items..." aria-label="Search">
      <button class="search" id="searchButton" type="button">
        <i class="fas fa-search"></i>
      </button>
    </div>

    <!-- Menu Items -->
    <div class="row row-cols-1 row-cols-lg-2 row-cols-xl-3 g-4">
    <%- include('admin/admin_card')%>
    
    </div>

  <!-- Bootstrap JS -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>
  <script src="./home.js"></script>
</body>
</html>
